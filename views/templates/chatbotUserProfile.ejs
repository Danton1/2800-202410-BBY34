<% if (user.profile_pic) { %>
    <img src="<%= user.profile_pic %>" alt="Profile img" class="w-full h-full rounded-full border border-sky-500 object-cover drop-shadow-xl text-black">
<% } else { %>
    <img src="defaultProfilePic.png" alt="Profile img" class="w-full h-full object-cover text-black">
<% } %>